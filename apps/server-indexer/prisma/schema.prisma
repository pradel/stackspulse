generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          String @id @map("id")
  blockHeight BigInt @map("block_height")
  timestamp   BigInt @map("timestamp")
}

