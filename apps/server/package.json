    "@t3-oss/env-core": "0.12.0",
    "better-sqlite3": "11.8.1",
    "db0": "0.2.4",
    "drizzle-orm": "0.39.3",
    "h3": "1.15.0",
    "nitro-cors": "0.7.1",
    "postgres": "3.4.5",
    "twitter-api-v2": "1.18.2",
    "unstorage": "1.14.4",
    "zod": "3.24.2",
    "zod-validation-error": "3.4.0"
